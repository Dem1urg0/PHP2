{% extends 'layouts/main.twig'%}
{% block style %}
    .sad h1 {
        font-size: 24px;
        margin-bottom: 10px;
        padding: 10px 0;
        border-bottom: 2px solid #4CAF50;
    }
    .sad a {
        text-decoration: none;
        color: #000;
    }
{% endblock %}
{% block content %}
<div class="sad">
    {% for user in users %}
        <h1><a href="/user/one?id={{ user.id }}">{{ user.login }}</a></h1>
    {% endfor %}
</div>
{% endblock %}